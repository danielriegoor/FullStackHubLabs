{
  "name": "dotf4p.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "docker:up": "docker compose -f docker/docker-compose.yml up -d",
    "docker:down": "docker compose -f docker/docker-compose.yml down",
    "db:migrate": "pnpm prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:seed": "pnpm prisma db seed",
    "db:reset": "pnpm prisma migrate reset --force && pnpm prisma db seed",
    "dx": "pnpm docker:up && pnpm db:generate && pnpm db:migrate && pnpm db:reset && pnpm dev"
  },
  "dependencies": {
    "@google/genai": "^1.40.0",
    "@google/generative-ai": "^0.24.1",
    "@prisma/adapter-pg": "^7.4.0",
    "@prisma/client": "^6.4.1",
    "axios": "^1.13.4",
    "cheerio": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.33.0",
    "jose": "^6.1.3",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "pg": "^8.18.0",
    "puppeteer": "^24.37.5",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "radix-ui": "^1.4.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwind-merge": "^3.4.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "dotenv": "^17.2.4",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "prisma": "^6.4.1",
    "shadcn": "^3.8.5",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  },
  "prisma": {
    "seed": "dotenv -e .env tsx src/db/seed.ts"
  }
}
